  <xs:complexType name="Range_Type">
    <xs:annotation>
      <xs:documentation>-Additional information sent to partner based on verbosity of IncInfoPlus attribute e.g payment ranges</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice>
        <xs:element name="LowerBoundUpperBound" type="BoundRange1_Type"/>
        <xs:element name="LowerUnboundUpperBound" type="BoundRange2_Type"/>
        <xs:element name="LowerBoundUpperUnbound" type="BoundRange3_Type"/>
        <xs:element name="LowerUnBoundUpperUnbound" type="BoundRange4_Type"/>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
