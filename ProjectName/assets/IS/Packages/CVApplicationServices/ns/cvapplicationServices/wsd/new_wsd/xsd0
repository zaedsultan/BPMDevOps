  <xs:complexType name="LoadRq_Type">
    <xs:annotation>
      <xs:documentation>-Billers may initiate a real-time bill upload operation at any time of their choosing by sending the LoadRq. 
							LoadRq restricts single bill at this point</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="GenerationDate" type="com:Date_Type"/>
      <xs:element name="Bill" type="bill:LoadBill_Type"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LoadRs_Type">
    <xs:annotation>
      <xs:documentation>-ping back bill info in case of success.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BillerId" type="com:BillerId_Type"/>
      <xs:element name="BillingAcct" type="com:BillingAcct_Type" minOccurs="0"/>
      <xs:element name="BillCategory" type="bill:BillCategory_Type" minOccurs="0"/>
      <xs:element name="ServiceType" type="com:ServiceType_Type" minOccurs="0"/>
      <xs:element name="BillNumber" type="com:BillNumber_Type" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
