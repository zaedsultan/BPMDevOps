<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd" version="2.2">
	<application>
		<message-bundle>caf.war.CVVerficationTasks.AppResources</message-bundle>
		<locale-config>
			<default-locale>en_US</default-locale>
		</locale-config>
	</application>



	<managed-bean>
		<managed-bean-name>TaskDisplayProvider</managed-bean-name>
		<managed-bean-class>com.webmethods.caf.faces.data.task.TaskDisplayProvider</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<description>Specifies whether the session bean expires when the pageflow is out of scope</description>
			<display-name>Expire With Page Flow</display-name>
			<property-name>expireWithPageFlow</property-name>
			<property-class>java.lang.Boolean</property-class>
			<value>true</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>TaskPriorityMapProvider</managed-bean-name>
		<managed-bean-class>com.webmethods.caf.faces.data.object.TableSelectItemGroupProvider</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<description>Specifies whether the session bean expires when the pageflow is out of scope</description>
			<display-name>Expire With Page Flow</display-name>
			<property-name>expireWithPageFlow</property-name>
			<property-class>java.lang.Boolean</property-class>
			<value>true</value>
		</managed-property>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>PublicSLANotificationxNotificationProvider</managed-bean-name>
		<managed-bean-class>com.webmethods.caf.faces.data.task.impl.TaskNotificationContentProvider</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>subscriptionID</property-name>
			<property-class>java.lang.String</property-class>
			<value>EA532B7E-81D8-A3C3-48F2-AF89D4B3381C</value>
		</managed-property>
		<managed-property>
			<property-name>subscriptionName</property-name>
			<property-class>java.lang.String</property-class>
			<value>PublicSLANotification</value>
		</managed-property>
		<managed-property>
			<property-name>notificationSubject</property-name>
			<property-class>java.lang.String</property-class>
			<value>#Subject</value>
		</managed-property>
		<managed-property>
			<property-name>taskClientBean</property-name>
			<property-class>com.webmethods.caf.faces.data.task.ITaskContentProvider</property-class>
			<value>#{currentTask}</value>
		</managed-property>
		<managed-property>
			<property-name>notificationBodyView</property-name>
			<property-class>java.lang.String</property-class>
			<value>/PublicSLANotification.xhtml</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>DateEventType</managed-bean-name>
		<managed-bean-class>com.webmethods.caf.faces.data.task.TaskBusinessCalendarModel</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>baseDateTime</property-name>
			<value>#{currentTask.taskInfo.createdDate}</value>
		</managed-property>
		<managed-property>
			<property-name>workdays</property-name>
			<property-class>int</property-class>
			<value>1</value>
		</managed-property>
		<managed-property>
			<property-name>workhours</property-name>
			<property-class>int</property-class>
			<value>0</value>
		</managed-property>
		<managed-property>
			<property-name>workminutes</property-name>
			<property-class>int</property-class>
			<value>0</value>
		</managed-property>
		<managed-property>
			<property-name>corporateCalendar</property-name>
			<null-value />
		</managed-property>
	</managed-bean></faces-config>
